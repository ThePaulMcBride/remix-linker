{
  "region": "lhr",
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "test.linker.paulmcbride.com"
        }
      ],
      "destination": "/page/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/(.*).linker.paulmcbride.com",
      "destination": "linker.paulmcbride.com/$1"
    }
  ]
}
